<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Login_SignUp.Verification">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="353dp"
        android:layout_height="379dp"
        android:layout_marginTop="4dp"
        android:clickable="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/otp_image" />

    <EditText
        android:id="@+id/phnno"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Otp"
        android:inputType="phone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.557"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.58" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/verify"
        style="?android:attr/borderlessButtonStyle"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="162dp"
        android:layout_marginEnd="156dp"
        android:layout_marginBottom="152dp"
        android:background="@drawable/signin_back"
        android:fontFamily="@font/nexabold"
        android:text="Verify"
        android:textAllCaps="false"
        android:textColor="@color/black"
        android:textSize="16dp"
        android:textStyle="bold"
        app:cornerRadius="12dp"
        app:backgroundTint="#f2f2f2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.705"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ph" />

    <EditText
        android:id="@+id/editTextTextPersonName5"
        android:layout_width="30dp"
        android:layout_height="42dp"
        android:layout_margin="6dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/opt_tb_back"
        android:ems="10"
        android:fontFamily="@font/nexalight"
        android:inputType="textPersonName"
        android:padding="5dp"
        android:text="4"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/editTextTextPersonName8"
        app:layout_constraintEnd_toStartOf="@+id/editTextTextPersonName7"
        app:layout_constraintTop_toTopOf="@+id/editTextTextPersonName8" />

    <EditText
        android:id="@+id/editTextTextPersonName7"
        android:layout_width="30dp"
        android:layout_height="42dp"
        android:layout_margin="6dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/opt_tb_back"
        android:ems="10"
        android:fontFamily="@font/nexalight"
        android:inputType="textPersonName"
        android:padding="5dp"
        android:text="5"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/editTextTextPersonName5"
        app:layout_constraintEnd_toStartOf="@+id/editTextTextPersonName6"
        app:layout_constraintTop_toTopOf="@+id/editTextTextPersonName5" />

    <EditText
        android:id="@+id/editTextTextPersonName3"
        android:layout_width="30dp"
        android:layout_height="42dp"
        android:layout_margin="6dp"
        android:background="@drawable/opt_tb_back"
        android:ems="10"
        android:fontFamily="@font/nexalight"
        android:inputType="phone"
        android:padding="5dp"
        android:text="1"
        android:textAlignment="center"
        app:layout_constraintEnd_toStartOf="@+id/editTextTextPersonName4"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="334dp" />

    <EditText
        android:id="@+id/editTextTextPersonName4"
        android:layout_width="30dp"
        android:layout_height="42dp"
        android:layout_margin="6dp"
        android:layout_marginEnd="4dp"
        android:background="@drawable/opt_tb_back"
        android:ems="10"
        android:fontFamily="@font/nexalight"
        android:inputType="textPersonName"
        android:padding="5dp"
        android:text="2"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/editTextTextPersonName3"
        app:layout_constraintEnd_toStartOf="@+id/editTextTextPersonName8"
        app:layout_constraintTop_toTopOf="@+id/editTextTextPersonName3" />

    <EditText
        android:id="@+id/editTextTextPersonName8"
        android:layout_width="30dp"
        android:layout_height="42dp"
        android:layout_margin="6dp"
        android:layout_marginEnd="12dp"
        android:background="@drawable/opt_tb_back"
        android:ems="10"
        android:fontFamily="@font/nexalight"
        android:inputType="textPersonName"
        android:padding="5dp"
        android:text="3"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/editTextTextPersonName4"
        app:layout_constraintEnd_toStartOf="@+id/editTextTextPersonName5"
        app:layout_constraintTop_toTopOf="@+id/editTextTextPersonName4" />

    <EditText
        android:id="@+id/editTextTextPersonName6"
        android:layout_width="30dp"
        android:layout_height="42dp"
        android:layout_margin="6dp"
        android:background="@drawable/opt_tb_back"
        android:ems="10"
        android:fontFamily="@font/nexalight"
        android:inputType="textPersonName"
        android:padding="5dp"
        android:text="6"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/editTextTextPersonName7"
        app:layout_constraintTop_toTopOf="@+id/editTextTextPersonName7"
        tools:layout_editor_absoluteX="292dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nexalight"
        android:text="Don't Receive Code?"
        android:textColor="#000000"
        android:textSize="18dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="453dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nexalight"
        android:text="Resend"
        android:textColor="#2BB3FF"
        android:textSize="18dp"
        app:layout_constraintEnd_toEndOf="@+id/textView3"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

</androidx.constraintlayout.widget.ConstraintLayout>